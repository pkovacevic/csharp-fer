@model StudentVm
@{
    ViewBag.Title = "Create Student";
}

<h1>@ViewBag.Title</h1>

<form asp-action="Create">

    @await Html.PartialAsync("FormInputPartial", Model)

    <div>
        <a class="btn btn-default" asp-action="Index">Cancel</a>
        <input type="submit" value="Save" class="btn btn-success" />
    </div>
</form>
